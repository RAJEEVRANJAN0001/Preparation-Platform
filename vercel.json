{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/PLACEMENT NOTES/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/pdf"
                },
                {
                    "key": "Content-Disposition",
                    "value": "inline"
                }
            ]
        }
    ]
}